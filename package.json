{
  "name": "@schedule-it/client",
  "version": "0.1.0",
  "private": true,
  "description": "Client for @schedule-it/server",
  "author": "Zaconly <thomas.vaucois@viacesi.fr>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix . --ignore-path ./.gitignore",
    "format": "prettier --write \"./src/**/*.{ts,tsx,css,md,json}\"",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.1.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "clsx": "^1.1.1",
    "graphql": "^15.3.0",
    "next": "11.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.3.0",
    "react-scrollbars-custom": "^4.0.25",
    "slugify": "^1.4.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.7",
    "@graphql-codegen/import-types-preset": "^1.17.7",
    "@graphql-codegen/typescript": "1.17.7",
    "@graphql-codegen/typescript-operations": "1.17.7",
    "@graphql-codegen/typescript-react-apollo": "1.17.7",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "yarn lint"
    ]
  }
}
